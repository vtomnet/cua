    "onnxruntime-web": "^1.23.0",
    "openai": "^6.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.14",
    "whisper-feature-extractor": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@types/bun": "latest",
    "concurrently": "^9.2.1",
    "electron": "^38.2.1",
    "electron-vite": "^4.0.1"
  }
